<view class="con" bindtouchstart="startFirst" bindtouchmove="moveFirst" bindtouchend="endFirst">
    <view class="">
        <view class="item" id="item-0-0" style="left:{{item_0_0_left[0]}}rpx;top:{{item_0_0_top[0]}}rpx"></view>
        <view class="item" id="item-0-1" style="left:{{item_0_0_left[1]}}rpx;top:{{item_0_0_top[0]}}rpx"></view>
        <view class="item" id="item-0-2" style="left:{{item_0_0_left[2]}}rpx;top:{{item_0_0_top[0]}}rpx"></view>
        <view class="item" id="item-0-3" style="left:{{item_0_0_left[3]}}rpx;top:{{item_0_0_top[0]}}rpx"></view>
        <view class="item" id="item-1-0" style="left:{{item_0_0_left[0]}}rpx;top:{{item_0_0_top[1]}}rpx"></view>
        <view class="item" id="item-1-1" style="left:{{item_0_0_left[1]}}rpx;top:{{item_0_0_top[1]}}rpx"></view>
        <view class="item" id="item-1-2" style="left:{{item_0_0_left[2]}}rpx;top:{{item_0_0_top[1]}}rpx"></view>
        <view class="item" id="item-1-3" style="left:{{item_0_0_left[3]}}rpx;top:{{item_0_0_top[1]}}rpx"></view>
        <view class="item" id="item-2-0" style="left:{{item_0_0_left[0]}}rpx;top:{{item_0_0_top[2]}}rpx"></view>
        <view class="item" id="item-2-1" style="left:{{item_0_0_left[1]}}rpx;top:{{item_0_0_top[2]}}rpx"></view>
        <view class="item" id="item-2-2" style="left:{{item_0_0_left[2]}}rpx;top:{{item_0_0_top[2]}}rpx"></view>
        <view class="item" id="item-2-3" style="left:{{item_0_0_left[3]}}rpx;top:{{item_0_0_top[2]}}rpx"></view>
        <view class="item" id="item-3-0" style="left:{{item_0_0_left[0]}}rpx;top:{{item_0_0_top[3]}}rpx"></view>
        <view class="item" id="item-3-1" style="left:{{item_0_0_left[1]}}rpx;top:{{item_0_0_top[3]}}rpx"></view>
        <view class="item" id="item-3-2" style="left:{{item_0_0_left[2]}}rpx;top:{{item_0_0_top[3]}}rpx"></view>
        <view class="item" id="item-3-3" style="left:{{item_0_0_left[3]}}rpx;top:{{item_0_0_top[3]}}rpx"></view>
    </view>

    <view class="movebox" style="left:{{left}}rpx;top:{{top}}rpx">{{n1}}</view>
    <view class="movebox" style="left:{{left2}}rpx;top:{{top2}}rpx">{{n2}}</view>
</view>
